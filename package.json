{
  "name": "graphql-boilerplate",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config server-load.js",
    "start:testFromLocal": "Prisma_Test=1 nodemon -e js,graphql -x node -r dotenv/config server-load.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground",
    "gomysql": "docker exec -it prisma_mysql_1 mysql -u root --host 127.0.0.1 --port 3306 --password=prisma",
    "deploy": "cd database && prisma deploy --force && cd ../ && graphql get-schema --project database",
    "deploy:test": "cd database_test && prisma deploy --force && cd ../ && graphql get-schema --project database_test"
  },
  "dependencies": {
    "apollo-server-cache-redis": "^0.1.0",
    "babel-plugin-import": "^1.8.0",
    "bcryptjs": "2.4.3",
    "graphql-cost-analysis": "^1.0.1",
    "graphql-request": "^1.6.0",
    "graphql-yoga": "latest",
    "jest": "^23.4.1",
    "jsonwebtoken": "latest",
    "prisma": "^1.12",
    "prisma-binding": "latest"
  },
  "devDependencies": {
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-parameters": "^6.16.0",
    "babel-plugin-transform-es2015-spread": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-plugin-transform-react-inline-elements": "^6.5.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.7",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.5.0",
    "dotenv": "6.0.0",
    "graphql-cli": "2.16.4",
    "module-resolver": "^1.0.0",
    "nodemon": "1.18.3",
    "npm-run-all": "4.1.3"
  }
}
